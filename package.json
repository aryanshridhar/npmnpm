{
  "name": "dyte-dependency-analyzer",
  "version": "0.0.0-development",
  "description": "A quick CLI tool to get outdated dependencies and update them if required",
  "files": [
    "!lib/**/*",
    "lib/**/*",
    "bin/**/*"
  ],
  "bin": {
    "dda": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-submissions/dyte-vit-2022-aryanshridhar.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aryan Shridhar",
    "email": "aryanshridhar7@gmail.com",
    "url": "https://github.com/aryanshridhar"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "boilerplate",
    "cli",
    "template",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitizen",
    "semantic-release",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/dyte-submissions/dyte-vit-2022-aryanshridhar/issues"
  },
  "homepage": "https://github.com/dyte-submissions/dyte-vit-2022-aryanshridhar#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "commander": "^7.2.0",
    "csvtojson": "^2.0.10",
    "default-branch": "^1.1.1",
    "ora": "^5.4.1",
    "parse-github-url": "^1.0.2",
    "semver": "^7.3.7",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.11",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.2",
    "codecov": "^3.8.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
